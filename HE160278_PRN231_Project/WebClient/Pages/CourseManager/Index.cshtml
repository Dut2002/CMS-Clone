@page
@using System.Security.Claims;
@model WebClient.Pages.CourseManager.IndexModel
@{
}

<div id="page" class="container-fluid">
	<div id="page-content" class="row">
		<div id="region-main-box" class="col-8">
			<section id="region-main">
				<div class="card">
					<div class="card-body">
						<span class="notifications" id="user-notifications"></span>
						<div role="main">
							<span id="maincontent"><a class="btn btn-primary" asp-page="/CourseManager/Create">Tạo khóa học mới</a></span>
							<span></span>
							<form id="coursesearch" asp-page="/CourseManager/Index" method="get">
								<fieldset class="coursesearchbox invisiblefieldset col-8">
									<label class="col-4" for="coursesearchbox">Tìm kiếm khoá học: </label>
									<input class="col-8" type="text" id="coursesearchbox" size="30" asp-for="Search" name="search" />
								</fieldset>
								<fieldset class="coursesearchbox invisiblefieldset col-8">
									<label class="col-4" for="coursesearchbox">Môn học </label>
									<select class="col-8" id="coursesearchbox" asp-for="Code" asp-items="@Model.Subjects">
										<option value="">-- Chọn một môn học --</option>
									</select>
								</fieldset>
								<fieldset class="coursesearchbox invisiblefieldset col-8">
									<input type="submit" value="Xem" />
								</fieldset>
							</form>
							<h2>Kết quả tìm kiếm: @Model.Courses.Count()</h2>
							<div class="courses course-search-result course-search-result-search">
								<div class=" row card-deck my-4">
									@foreach (var course in Model.Courses)
									{
										<div class="card mb-3 course-card-view">
											<div class="" data-courseid="5097" data-type="1">
												<a asp-page="/CourseManager/Detail" asp-route-courseId="@course.CourseId" class="course-card-img">
													<div style="background-image:url(&#039;data:image/svg+xml;base64,&#039;)" class="card-img-top">
														@course.CourseName
													</div>
												</a>
												<div class="card-body">
													<h5 class='card-title text-center m-1'>
														<a class="" asp-page="/CourseManager/Detail" asp-route-courseId="@course.CourseId">
															@course.Semester @course.Year - Subject:
															<span class="highlight">@course.Code</span> - Lecturer: @course.Creator - Course Name: @course.CourseName
														</a>
													</h5>
												</div>
												<div class="card-block text-center">
													<i class="icon fa fa-sign-in fa-fw " title="Self enrolment" aria-label="Self enrolment">

													</i><div class="pull-right"></div>
												</div><div class="card-footer teachers">
													<ul>
														@foreach (var teacher in course.Managers)
														{
															<li>
																Teacher:
																<a href="#">
																	@teacher.FirstName @teacher.LastName;
																</a>
															</li>
														}
													</ul>
												</div>
												<div class="coursecat text-center">
													Mục:
													<a class="" asp-page="/CourseManager/Index" asp-route-code="@course.Code">@course.SubjectName (@course.Code)</a>
												</div>

												<div id="fgroup_id_buttonar" class="form-group row  fitem femptylabel  " data-groupname="buttonar">
													@if (course.CreatorId == int.Parse(User.FindFirstValue("AccountId")))
													{
														<div class="col-md-12 form-inline felement" data-fieldtype="group">
															<a href="/CourseManager/Update?courseId=@course.CourseId" class="btn btn-primary">Update</a>

															<form asp-page="/CourseManager/Index" id="deleteForm-@course.CourseId" method="post">
																<input type="hidden" value="@course.CourseId" name="courseId" />
																<button class="btn btn-secondary" onclick="confirmDelete(@course.CourseId)" type="button">Delete</button>
															</form>
														</div>

													}
												</div>
											</div>
										</div>

									}
								</div>
							</div><div class="box generalbox mdl-align py-3">
							</div>
						</div>
					</div>
				</div>
			</section>
		</div>
	</div>
</div>

<script>
	function confirmDelete(courseId) {
		if (confirm("Bạn có chắc chắn muốn xóa khóa học này?")) {
			document.getElementById("deleteForm-"+courseId).submit();
		}
	}
</script>